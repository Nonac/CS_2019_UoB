@article{Lee, 
  author = {Gaspers, Serge and Lee, Edward}, 
  title = {{Faster Graph Coloring in Polynomial Space}}, 
  doi = {Lee}, 
  eprint = {1607.06201}, 
  abstract = {{We present a polynomial-space algorithm that computes the number independent sets of any input graph in time \$O(1.1387\textasciicircumn)\$ for graphs with maximum degree 3 and in time \$O(1.2355\textasciicircumn)\$ for general graphs, where n is the number of vertices. Together with the inclusion-exclusion approach of Bj\textbackslash"orklund, Husfeldt, and Koivisto [SIAM J. Comput. 2009], this leads to a faster polynomial-space algorithm for the graph coloring problem with running time \$O(2.2355\textasciicircumn)\$. As a byproduct, we also obtain an exponential-space \$O(1.2330\textasciicircumn)\$ time algorithm for counting independent sets. Our main algorithm counts independent sets in graphs with maximum degree 3 and no vertex with three neighbors of degree 3. This polynomial-space algorithm is analyzed using the recently introduced Separate, Measure and Conquer approach [Gaspers \& Sorkin, ICALP 2015]. Using Wahlstr\textbackslash"om's compound measure approach, this improvement in running time for small degree graphs is then bootstrapped to larger degrees, giving the improvement for general graphs. Combining both approaches leads to some inflexibility in choosing vertices to branch on for the small-degree cases, which we counter by structural graph properties.}}, 
  journal = {arXiv}, 
  local-url = {file://localhost/D:\Papers%20Library\Gaspers-Faster%20Graph%20Coloring%20in%20Polynomial%20Space-2016-arXiv.pdf}, 
  year = {2016}
}
@article{undefined, 
  author = {Junosza-Szaniawski, Konstanty and Tuczynski, Michal}, 
  title = {{Counting independent sets via Divide Measure and Conquer method}}, 
  eprint = {1503.08323}, 
  abstract = {{In this paper we give an algorithm for counting the number of all independent sets in a given graph which works in time \$O\textasciicircum*(1.1394\textasciicircumn)\$ for subcubic graphs and in time \$O\textasciicircum*(1.2369\textasciicircumn)\$ for general graphs, where \$n\$ is the number of vertices in the instance graph, and polynomial space. The result comes from combining two well known methods "Divide and Conquer" and "Measure and Conquer". We introduce this new concept of Divide, Measure and Conquer method and expect it will find applications in other problems. The algorithm of Bj\textbackslash"orklund, Husfeldt and Koivisto for graph colouring with our algorithm used as a subroutine has complexity \$O\textasciicircum*(2.2369\textasciicircumn)\$ and is currently the fastest graph colouring algorithm in polynomial space.}}, 
  journal = {arXiv}, 
  year = {2015}
}
